<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - My ASP.NET Application</title>
    @Styles.Render("~/Content/MaterialAdminVendorCss")
    @Styles.Render("~/Content/MaterialAdminCss")
</head>
<body>
    @Html.Partial("_HeaderPartial")
    <!---->
    <section id="main">
        @Html.Partial("_SideBarPartial")
        <section id="content">
            <div class="container">
                @RenderBody()
            </div>
        </section>
    </section>

    @Html.Partial("_FooterPartial")

    <!-- Page Loader -->
    <div class="page-loader">
        <div class="preloader pls-blue">
            <svg class="pl-circular" viewBox="25 25 50 50">
                <circle class="plc-path" cx="50" cy="50" r="20" />
            </svg>

            <p>Please wait...</p>
        </div>
    </div>

    @Scripts.Render("~/bundles/MaterialAdminJquery")
    @Scripts.Render("~/bundles/MaterialAdminBootstrap")
    @Scripts.Render("~/bundles/MaterialAdminCustom")
    @RenderSection("scripts", required: false)
</body>
</html>
